<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DictionaryServlet</servlet-name>
  	<display-name>DictionaryServlet</display-name>
  	<description></description>
  	<servlet-class>com.bh08.hungarian_spanish_dictionary.servlets.DictionaryServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TranslationServlet</servlet-name>
  	<display-name>TranslationServlet</display-name>
  	<description></description>
  	<servlet-class>com.bh08.hungarian_spanish_dictionary.servlets.TranslationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DictionaryServlet</servlet-name>
  	<url-pattern>/DictionaryServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TranslationServlet</servlet-name>
  	<url-pattern>/TranslationServlet</url-pattern>
  </servlet-mapping>
</web-app>
